{
	"name": "discord-faucet-bot",
	"version": "2.0.0",
	"description": "Discord Faucet Bot to let people request for Testnet tokens",
	"main": "index.js",
	"author": "Yanuka Deneth",
	"engines": {
		"node": ">=16.9.0"
	},
	"scripts": {
		"lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
		"lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"start": "ts-node src/index.ts",
		"start:dev": "ts-node-dev src/index.ts",
		"start:prod": "tsc && node dist/deploy-commands.js && node dist/index.js",
		"build": "tsc",
		"bot:addcommands": "ts-node src/deploy-commands.ts",
		"bot:deletecommands": "ts-node src/delete-commands.ts",
		"test": "mocha -r ts-node/register ./src/tests/*.spec.ts",
		"typechain": "typechain --target ethers-v5 --out-dir ./src/types/ethers-contracts '../token-vesting-contracts/artifacts/src/TokenVesting.sol/TokenVesting.json'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yanukadeneth99/Discord-Faucet-Bot.git"
	},
	"bugs": {
		"url": "https://github.com/yanukadeneth99/Discord-Faucet-Bot/issues"
	},
	"homepage": "https://github.com/yanukadeneth99/Discord-Faucet-Bot#readme",
	"license": "MIT",
	"keywords": [
		"faucet",
		"bot",
		"learnweb3dao",
		"learnweb3"
	],
	"dependencies": {
		"@celo-tools/celo-ethers-wrapper": "^0.3.0",
		"@discordjs/rest": "^1.3.0",
		"@ethersproject/abi": "^5.7.0",
		"@ethersproject/providers": "^5.7.2",
		"@types/sinon": "^10.0.16",
		"discord-api-types": "*",
		"discord.js": "*",
		"dotenv": "^16.0.3",
		"ethers": "^5.7.2",
		"keyv": "^4.5.2",
		"pg": "^8.8.0",
		"reflect-metadata": "^0.1.13",
		"sinon": "^16.0.0",
		"typeorm": "^0.3.10"
	},
	"devDependencies": {
		"@typechain/ethers-v5": "^11.1.1",
		"@types/chai": "^4.3.6",
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"chai": "^4.3.8",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^1.0.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-unicorn": "^44.0.2",
		"mocha": "^10.2.0",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"tslib": "^2.4.1",
		"typechain": "^8.3.1",
		"typescript": "^4.8.4"
	}
}
